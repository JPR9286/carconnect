<h1>DASHBOARD USER</h1>

<br>
<div class="border">
  <h2>AFFICHAGE VEHICULES </h2>
   <% @vehicules.each do |vehicule| %>
    <p><%= vehicule.model %></p>
    <p><%= vehicule.description %></p>
    <p><%= vehicule.prix %></p>
  <% end %>
</div>
<br>
<div class="border">
  <h2>affichage bookings</h2>
  <% @bookings.each do |booking| %>
    <p><%= booking.vehicule.model %>, <%= booking.date_debut %>, <%= booking.date_fin %></p>
  <% end %>
</div>


<br>
<%= link_to "Home page", root_path %>
